<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>  
        <!--给实体类起一个别名 user -->  
        <typeAlias type="org.ack.cfs.entity.User" alias="User" />  
        
    </typeAliases>
    <!-- 分页拦截器 -->
    <plugins>
		<plugin interceptor="org.ack.cfs.intercept.PageIntercepter">
			<property name="dataBaseType" value="mysql"/><!-- 设置方言 -->
		</plugin>
	</plugins>
    
</configuration>